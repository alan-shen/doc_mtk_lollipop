+static void
+restore_rom(int confirm, Device* device, const char* title[]) {
+	if (confirm) {
+        char** headers = NULL;
+        const char** title_headers = NULL;
+
+        if (title_headers == NULL) {
+            const char* def_title[] = { "Confirm restore the factory ROM?",
+                                      "  THIS CAN NOT BE UNDONE.",
+                                      "",
+                                      NULL };
+            if (title == NULL) {
+                headers = (char **)def_title;
+            } else {
+                headers = (char **)title;
+            }
+            title_headers = prepend_title((const char**)headers);
+        }
+
+        const char* items[] = { " No",
+                                " No",
+                                " No",
+                                " No",
+                                " No",
+                                " No",
+                                " No",
+                                " Yes -- restore factory ROM",   // [7]
+                                " No",
+                                " No",
+                                " No",
+                                NULL };
+
+        if(gGraphicMode){
+            GraphicScreenRecoveryUI * gUi = (GraphicScreenRecoveryUI *)ui;
+            gUi->setMenuHeadersMap(title_headers, GraphicScreenRecoveryUI::HEADERS_RESTORE_ROM);
+        }
+
+        int chosen_item = get_menu_selection(title_headers, items, 1, 0, device);
+        if (chosen_item != 7) {
+            return;
+        }
+    }
+
+    ui->Print("\n-- Install %s ...\n", RESTORE_ROM_FILE);
+    int wipe_cache = 0;
+    int status = 0;
+    set_sdcard_update_bootloader_message();
+    //instaboot_disable();
+    //instaboot_clear();
+    status = install_package(RESTORE_ROM_FILE, &wipe_cache, TEMPORARY_INSTALL_FILE, true);
+    if (status >= 0) {
+        if (status != INSTALL_SUCCESS) {
+            ui->SetBackground(RecoveryUI::ERROR);
+            ui->Print("Installation aborted.\n");
+        } else if (!ui->IsTextVisible()) {
+            return;   // reboot if logs aren't visible
+        } else {
+            erase_volume("/data");
+            erase_volume("/cache");
+            /* reset to default environment */
+            //set_bootloader_env("upgrade_step", "1");
+            ui->Print("Restore ROM complete.\n");
+        }
+    }
+    ensure_path_unmounted(RESTORE_ROM_FILE);
+    sync();  // For good measure.
+}
